<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneChartMenu" prefHeight="163.0" prefWidth="763.0" stylesheets="style.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChartMenuController">
   <children>
      <Button fx:id="btnAbsoluteError" layoutX="30.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleAbsoluteError" stylesheets="@style.css" text="Absolute error" />
      <Button fx:id="btnStandardDeviaton" layoutX="181.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleStandardDeviation" prefHeight="23.0" prefWidth="177.0" stylesheets="@style.css" text="Standard deviation" />
      <Button fx:id="btnTimeConsumed" layoutX="382.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleTimeConsumed" prefHeight="23.0" prefWidth="185.0" stylesheets="@style.css" text="Time consumed" />
      <Label fx:id="labelChooseChart" layoutX="30.0" layoutY="25.0" prefHeight="50.0" prefWidth="240.0" text="Choose a chart:" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Georgia" size="21.0" />
         </font>
      </Label>
      <Button fx:id="btnAll" layoutX="582.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleAll" prefHeight="23.0" prefWidth="158.0" stylesheets="@style.css" text="err/stddev" />
   </children>
</AnchorPane>
